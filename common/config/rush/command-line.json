{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/command-line.schema.json",
  "commands": [
    {
      "name": "format",
      "commandKind": "bulk",
      "summary": "Exec all packages format",
      "safeForSimultaneousRushProcesses": true,
      "enableParallelism": false,
      "shellCommand": "npm run format"
    },
    {
      "name": "package",
      "commandKind": "bulk",
      "summary": "Exec all packages pack",
      "safeForSimultaneousRushProcesses": true,
      "enableParallelism": false,
      "shellCommand": "npm run pack"
    },
    {
      "commandKind": "bulk",
      "name": "test",
      "summary": "Exec all packages tests",
      "enableParallelism": false,
      "ignoreMissingScript": true,
      "safeForSimultaneousRushProcesses": true
    }
  ]
}
