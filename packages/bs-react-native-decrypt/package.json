  "license": "MIT",
  "homepage": "https://github.com/CityOfZion/blockchain-services",
  "devDependencies": {
    "@types/react": "17.0.21",
    "@types/react-native": "0.70.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "pod-install": "^0.1.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-builder-bob": "^0.21.3",
    "turbo": "^1.10.7",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
